<h1>Log In</h1>
<%=form_tag log_in_path do%>
  <%=label_tag :email %>
  <%=text_field_tag :email %>
  <%=label_tag :password %>
  <%=password_field_tag :password %>
<%=submit_tag "Log in" %>
<%end%>
<%= button_to "Sign Up",action="users/new",method: :get%>
<p><%= button_to "Continue as guest",log_in_path, options = { method: :post,name:"guest"} %></p>
